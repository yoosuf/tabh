{
    "name": "finetick/quicksilver",
    "description": "The shopping agrecator.",
    "keywords": ["cart", "shopping", "quicksilver", "telenor", "tonic"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "cviebrock/eloquent-sluggable": "^4.3",
        "rtconner/laravel-tagging": "~2.2",
        "gloudemans/shoppingcart": "^2.4",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "fzaninotto/faker": "^1.7"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
